<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="android:Theme.Light" />

    <style name="user_detail_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#535353</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">XXX</item>
    </style>

    <style name="DashboardButton">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/accent_1</item>
        <item name="android:background">@null</item>
    </style>

    <style name="WhatsOnItemBase">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/body_text_1</item>
        <item name="android:maxLines">2</item>
        <item name="android:paddingLeft">@dimen/whats_on_item_padding</item>
        <item name="android:paddingRight">@dimen/whats_on_item_padding</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="WhatsOnStaticItem" parent="style/WhatsOnItemBase">
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/body_text_1</item>
        <item name="android:maxLines">2</item>
    </style>

    <style name="WhatsOnButtonItem" parent="style/WhatsOnStaticItem">
        <item name="android:textColor">@color/accent_1</item>
        <item name="android:background">@drawable/whats_on_button</item>
        <item name="android:paddingRight">@dimen/whats_on_button_right_padding</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="GreenDroid" />

    <style name="GreenDroid.Widget" parent="@style/GreenDroid" />

    <style name="GreenDroid.Widget.ActionBar" parent="@style/GreenDroid.Widget">
        <item name="android:background">?gdActionBarBackground</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/gd_action_bar_height</item>
        <item name="dividerDrawable">?gdActionBarDividerDrawable</item>
        <item name="dividerWidth">?gdActionBarDividerWidth</item>
        <item name="homeDrawable">?gdActionBarHomeDrawable</item>
        <item name="maxItems">?gdActionBarMaxItems</item>
    </style>

    <style name="GreenDroid.Widget.ActionBar.Dashboard" parent="@style/GreenDroid.Widget.ActionBar">
        <item name="type">dashboard</item>
    </style>

    <style name="GreenDroid.Widget.ActionBar.Empty" parent="@style/GreenDroid.Widget.ActionBar">
        <item name="type">empty</item>
    </style>

    <style name="GreenDroid.Widget.ActionBar.Item" parent="@style/GreenDroid.Widget">
        <item name="android:background">?gdActionBarItemBackground</item>
        <item name="android:layout_width">@dimen/gd_action_bar_height</item>
    </style>

    <style name="GreenDroid.Widget.ActionBar.Title" parent="@style/GreenDroid.Widget">
        <item name="android:textColor">?gdActionBarTitleColor</item>
    </style>

    <style name="Theme.GreenDroid" parent="@android:style/Theme">
        <item name="gdActionBarTitleColor">@android:color/white</item>
        <item name="gdActionBarBackground">@color/gd_action_bar_tint</item>
